{
  "name": "koa-neo4j",
  "version": "1.3.0",
  "description": "Rapidly create REST APIs, powered by Koa and Neo4j -- batteries included with built-in role based authentication via JWT and reusable backend components",
  "main": "index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src --fix",
    "dev": "./node_modules/.bin/webpack --progress --colors --watch",
    "prebuild": "npm run lint",
    "build": "./node_modules/.bin/babel src -d dist && ./node_modules/.bin/webpack && ./node_modules/.bin/webpack --config webpack.config.jasmine.js",
    "test": "npm run build && NEO4J_PASSWORD=neo4j node jasmine.runner.js",
    "prepublish": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:yrong/koa-neo4j.git"
  },
  "keywords": [
    "webservice",
    "server",
    "API",
    "REST",
    "database",
    "graph",
    "neo4j",
    "koa"
  ],
  "author": "Keyvan Mir Mohammad Sadeghi <keyvan.m.sadeghi@gmail.com>",
  "contributors": [
    {
      "name": "Ron Yang",
      "email": "yrong1997@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yrong/koa-neo4j/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "jasmine": "^2.5.0",
    "jasmine-given": "^2.6.4",
    "jasmine-spec-reporter": "^2.7.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "chalk": "^2.4.2",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "log4js-wrapper-advanced": "^1.0.5",
    "neo4j-driver": "^1.7.5",
    "parse-neo4j": "^0.6.11",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  }
}
